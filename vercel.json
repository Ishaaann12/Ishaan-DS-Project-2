{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        }
    ],
    "installCommand": "pip install --no-cache-dir --disable-pip-version-check -r requirements.txt"
}
