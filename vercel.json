{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/main.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt && pip install pandas"
  }
  
