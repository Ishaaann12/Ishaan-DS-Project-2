{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        }
    ],
    "buildCommand": "pip install --no-cache-dir -r requirements.txt"
}

